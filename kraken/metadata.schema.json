      "$id": "#/properties/name",
      "type": "string",
      "title": "Filename of the model",
      "pattern": "^(.*)$"
    },
    "graphemes": {
      "$id": "#/properties/graphemes",
      "type": "array",
      "title": "Code points recognizable by the model",
      "uniqueItems": true,
      "minItems": 1,
      "items": {
        "$id": "#/properties/graphemes/items",
        "type": "string",
        "pattern": "^(.*)$"
      }
    }
  }
}
