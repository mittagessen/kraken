			{% for segment in line.recognition %}
				<span class="ocrx_word" id="segment_{{ segment.index }}" title="bbox {{ segment.bbox|join(' ') }}, x_conf {{ segment.confidences|join(' ') }}">{{ segment.text }}</span>
			{% endfor %}
			</span>
			<br/>
			{% endfor %}

